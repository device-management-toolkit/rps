{
  "name": "rcs_microservice",
  "version": "1.0.0",
  "description": "AMT Remote configuration service",
  "main": "index.js",
  "scripts": {
    "build": "tsc --w",
    "compile": "tsc",
    "start": "tsc && ts-node ./dist/Index.js",
    "devx": "RPS_NODE_ENV=dev RPS_USE_VAULT=false RPS_USE_DB_PROFILES=false RPS_LOG_LEVEL=silly npm start",
    "dev": "set RPS_NODE_ENV=dev&&set RPS_USE_VAULT=false&&set RPS_USE_DB_PROFILES=false&&set RPS_LOG_LEVEL=silly&& npm start",
    "test": "jest --coverage",
    "watch": "nodemon -e ts -w ./src -x npm run start",
    "lint": "eslint . --ext .ts"
  },
